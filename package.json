{
  "name": "@fwrlines/swatch",
  "author": "Adrian V.",
  "version": "1.0.0-alpha.4",
  "description": "",
  "repository": "github:fwrlines/swatch",
  "sideEffects": true,
  "scripts": {
    "storybook": "start-storybook -p 6007 -s ./src/assets/images,./src/assets/favicon",
    "build-storybook": "rm -rf ./public/* && build-storybook -c .storybook -o public --quiet -s ./src/assets/images,./src/assets/favicon",
    "build": "echo 0",
    "now-build": "mkdir ./public && npm run build-storybook --quiet",
    "pub": "npm version patch && npm publish",
    "compile": "sass main.scss main.css -c --no-source-map && postcss main.css -o main.min.css",
    "compile:custom": "sass custom.scss custom.css -c --no-source-map && postcss custom.css -o custom.min.css",
    "commit": "git add . && git commit -m 'Version boump' && git push",
    "version:alpha": "npm version prerelease --preid=alpha",
    "version:beta": "npm version prerelease --preid=beta"
  },
  "keywords": [],
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@storybook/addon-docs": "^6.0.0-beta.26",
    "@storybook/addon-links": "^6.0.0-beta.26",
    "@storybook/addons": "^6.0.0-beta.26",
    "@storybook/react": "^6.0.0-beta.26",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.5.3",
    "css-mqpacker": "^7.0.0",
    "cssnano": "^4.1.10",
    "mini-css-extract-plugin": "^0.9.0",
    "postcss-cli": "^7.1.1",
    "postcss-preset-env": "^6.7.0",
    "postcss-scss": "^2.1.1",
    "sass": "^1.26.3",
    "sass-loader": "^8.0.2",
    "stylelint": "^13.2.1",
    "stylelint-config-palantir": "^4.0.1",
    "stylelint-scss": "^3.16.0"
  },
  "dependencies": {}
}
