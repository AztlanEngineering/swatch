{
  "name": "@fwrlines/swatch",
  "author": "Adrian V.",
  "version": "1.0.0-beta.0",
  "description": "",
  "repository": "github:fwrlines/swatch",
  "sideEffects": true,
  "scripts": {
    "storybook": "start-storybook -p 6007 -s ./src/assets/images,./src/assets/favicon",
    "build-storybook": "rm -rf ./public/* && build-storybook -c .storybook -o public --quiet -s ./src/assets/images,./src/assets/favicon",
    "build": "echo 0",
    "now-build": "mkdir ./public && npm run build-storybook --quiet",
    "compile:main":"sass main.scss main.css -c --no-source-map",
    "compile:custom":"sass custom.scss custom.css -c --no-source-map",
    "minify:main":"postcss main.css -o main.min.css",
    "minify:custom":"postcss custom.css -o custom.min.css",
    "build:main": "npm run compile:main && npm run minify:main",
    "build:custom": "npm run compile:custom && npm run minify:custom",
    "commit": "git add . && git commit -m 'Version boump' && git push",
    "version:alpha": "npm version prerelease --preid=alpha",
    "version:beta": "npm version prerelease --preid=beta"
  },
  "keywords": [],
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@storybook/addon-docs": "^6.0.28",
    "@storybook/addon-links": "^6.0.28",
    "@storybook/addons": "^6.0.28",
    "@storybook/react": "^6.0.28",
    "babel-loader": "^8.1.0",
    "css-loader": "^5.0.0",
    "css-mqpacker": "^7.0.0",
    "cssnano": "^4.1.10",
    "mini-css-extract-plugin": "^1.2.1",
    "postcss-cli": "^8.2.0",
    "postcss-preset-env": "^6.7.0",
    "postcss-scss": "^3.0.2",
    "sass": "^1.28.0",
    "sass-loader": "^10.0.4",
    "stylelint": "^13.7.2",
    "stylelint-config-palantir": "^5.0.0",
    "stylelint-scss": "^3.18.0"
  },
  "dependencies": {}
}
